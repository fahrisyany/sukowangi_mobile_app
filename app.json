{
  "expo": {
    "name": "Sukowangi",
    "slug": "sukowangi_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon-2.png",
    "scheme": "sukowangi",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#044769"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-2.png",
        "backgroundColor": "#044769"
      },
      "package": "com.sukowangi.mobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
