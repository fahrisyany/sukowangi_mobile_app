{
  "expo": {
    "name": "sukowangi_mobile_app",
    "slug": "sukowangi_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon-2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#044769"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-2.png",
        "backgroundColor": "#044769"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
